package gameComponenets;

public class Button extends Rectangle {
	private Runnable doesSomething;

	public Button(Coordinate topLeft, double width, double height, Runnable doesSomething) {
		super(topLeft, width, height, RectangleType.LevelButton);
		this.doesSomething=doesSomething;
	}

	public Runnable getDoesSomething(){
		return doesSomething;
	}

}
