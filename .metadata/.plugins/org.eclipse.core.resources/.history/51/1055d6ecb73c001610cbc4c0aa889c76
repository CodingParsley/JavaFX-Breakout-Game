package application;

public class Test {
	public static void run(){
		Rectangle testR = new Rectangle(new Coordinate(0,0), 100,100);
		LineSegment testL = new LineSegment(new Coordinate(-10,50),testR.getCenterCoordinate());
		System.out.println("L"+testL.intersects(testR.getLeftLineSegment()));
		System.out.println("R"+testL.intersects(testR.getRightLineSegment()));
		System.out.println("B"+testL.intersects(testR.getBottomLineSegement()));
		System.out.println("T"+testL.intersects(testR.getTopLineSegment()));
	}
	public static void main(String[] args){
		run();
	}
}
