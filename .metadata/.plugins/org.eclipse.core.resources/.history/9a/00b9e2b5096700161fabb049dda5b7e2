package gameComponents;

public class DetectionTest {
	public static void main(String[] args){
		Rectangle r1 = new Rectangle(new Coordinate(0,0),4,4,RectangleType.Ball);
		Rectangle r2 = new Rectangle(new Coordinate(10,10),2,2,RectangleType.Ball);
		JavaCollisionDetector detector = new JavaCollisionDetector();
		if(detector.basicIntersects(r1, r2)){
			System.out.println("inside works");
		}
	}
}
