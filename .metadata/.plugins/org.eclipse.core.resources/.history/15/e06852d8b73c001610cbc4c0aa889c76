package application;

public class Test {
	public static void run(){
		Rectangle testR = new Rectangle(new Coordinate(0,0), 100,100);
		LineSegment testL = new LineSegment(new Coordinate(-10,50),testR.getCenterCoordinate());
		System.out.println(testL.intersects(testR.getLeftLineSegment()));
	}
	public static void main(String[] args){
		run();
	}
}
