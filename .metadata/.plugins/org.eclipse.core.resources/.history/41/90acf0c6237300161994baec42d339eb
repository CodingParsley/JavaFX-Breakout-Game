package gameComponents;

import javafx.event.Event;
import javafx.event.EventHandler;

// This class will add a listener to a viewModel rectangle and whenever that rectangle becomes unstoppable,
// This animation will animate the ball
public class UnstoppableAnimationHandler implements EventHandler{

	@Override
	public void handle(Event arg0) {
		if(arg0.getSource() instanceof Rectangle){
			System.out.println("hey");
		}

	}

}
