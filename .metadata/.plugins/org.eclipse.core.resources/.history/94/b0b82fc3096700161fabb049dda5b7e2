package gameComponents;

public class DetectionTest {
	public static void main(String[] args){
		Rectangle r1 = new Rectangle(new Coordinate(0,0),6,6,RectangleType.Ball);
		Rectangle r2 = new Rectangle(new Coordinate(2,2),2,2,RectangleType.Ball);
		JavaCollisionDetector detector = new JavaCollisionDetector();
		if(detector.basicIntersects(r1, r2)){
			System.out.println("inside works");
		}
	}
}
